{
  "manifestVersion": 1,
  "id": "azure-devops-impact-metrics",
  "name": "Azure DevOps Impact Metrics",
  "version": "1.0.9",
  "publisher": "JohnSeong97",
  "description": "Analyze your development impact across Azure DevOps repositories, pull requests, and work items",
  "public": true,
  "categories": [
    "Azure Boards",
    "Azure Repos"
  ],
  "tags": [
    "metrics",
    "analytics",
    "productivity",
    "reporting",
    "dashboard"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "branding": {
    "color": "rgb(34, 34, 34)",
    "theme": "dark"
  },
  "contributions": [
    {
      "id": "impact-metrics-hub",
      "type": "ms.vss-web.hub",
      "description": "View your development impact metrics",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "name": "Impact Metrics",
        "order": 99,
        "uri": "static/main.html",
        "icon": "bowtie-icon bowtie-math"
      }
    }
  ],
  "files": [
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "README.md",
      "addressable": true
    }
  ],
  "scopes": [
    "vso.code",
    "vso.work"
  ]
}
